---
show: true
list-components:
  'Component 1': 'docs/partials/elements/link/menu.html'
  'Component 2':
    'Component 2.1': 'docs/partials/elements/link/menu.html'
    'Component 2.2': '#component-2-2'
    'Component 2.3':
      'Component 2.3.1': '#component-1-3-1'
      'Component 2.3.2': '#component-1-3-2'
      'Component 2.3.3': '#component-1-3-3'
list-fragments:
  'Fragment 1': '#fragment-1'
list-elements:
  'Element 1': '#element-1'
list-packages:
  'Package 1': '#package-1'
---
<div class="partial controller {{#if show}} show{{/if}}" data-package="agency-pkg-docs" data-partial="fragments/menu" data-controller="agency-pkg-docs/partials/fragments/Menu" data-target="[data-package='agency-pkg-docs'][data-partial='components/overview']">
  <span class="js-click-toggle toggle">Show/Hide</span>
  <div>
    {{#block "content"}}
      <h3>Components</h3>
      {{{mixin "agency-pkg-docs/fragments/accordeon" list=list-components}}}

      <h3>Fragments</h3>
      {{{mixin "agency-pkg-docs/fragments/accordeon" list=list-fragments}}}

      <h3>Elements</h3>
      {{{mixin "agency-pkg-docs/fragments/accordeon" list=list-elements}}}

      <h3>Packages</h3>
      {{{mixin "agency-pkg-docs/fragments/accordeon" list=list-packages}}}
    {{/block}}
  </div>
</div>
