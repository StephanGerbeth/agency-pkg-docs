---
layout: "agency-pkg-layouts/documentation"
components:
  headline:
    text: 'Components'
fragments:
  headline:
    text: 'Fragments'
elements:
  headline:
    text: 'Elements'
packages:
  headline:
    text: 'Packages'
---
{{#mixin "agency-pkg-docs/overview"}}

  {{#content "menu"}}

      {{#mixin "agency-pkg-docs/overview/menu"}}

        {{#content "content"}}

          <div class="components">
            {{{mixin "agency-pkg-docs/overview/headline" ../../components.headline}}}
            {{#globTree "src/tmpl/partials/components/**/*.hbs"}}
              {{#each this}}
                {{{mixin "agency-pkg-docs/accordeon" this}}}
              {{/each}}
            {{/globTree}}
          </div>

          <div class="fragments">
            {{{mixin "agency-pkg-docs/overview/headline" ../../fragments.headline}}}
            {{#globTree "src/tmpl/partials/fragments/**/*.hbs"}}
              {{#each this}}
                {{{mixin "agency-pkg-docs/accordeon" this}}}
              {{/each}}
            {{/globTree}}
          </div>

          <div class="elements">
            {{{mixin "agency-pkg-docs/overview/headline" ../../elements.headline}}}
            {{#globTree "src/tmpl/partials/elements/**/*.hbs"}}
              {{#each this}}
                {{{mixin "agency-pkg-docs/accordeon" this}}}
              {{/each}}
            {{/globTree}}
          </div>

          <div class="packages">
            {{{mixin "agency-pkg-docs/overview/headline" ../../packages.headline}}}
            {{#globTree "node_modules/*-pkg-*/**/*.hbs"}}
              {{#each this}}
                {{{mixin "agency-pkg-docs/accordeon" this}}}
              {{/each}}
            {{/globTree}}
          </div>

        {{/content}}

      {{/mixin}}

  {{/content}}

{{/mixin}}
