.partial[data-package="agency-pkg-docs"][data-partial="elements/link/external"] {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  display: block;
  color: #5588ff;
  &:hover {
    color: #ff3344;
  }
  font-size: calc(11 / 16)em;
  line-height: 1;
  text-decoration: none;
  padding: calc(5 / 16)em calc(10 / 16)em;
}
