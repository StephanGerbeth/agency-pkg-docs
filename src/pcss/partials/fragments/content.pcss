.partial[data-package="agency-pkg-docs"][data-partial="fragments/content"] {
  position: relative;
  bottom: 0;
  height: 100%;
  iframe {
    display: block;
    border: none;
    width: 100%;
    height: 100%;
    will-change: filter;
    transition: filter .35s ease;
    .js-show-menu & {
      filter: blur(8px);
    }
  }
}
