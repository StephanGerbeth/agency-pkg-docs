---
src:
  headline:
    text: 'src'
  list:
    'Component 1': 'docs/partials/elements/link/menu.html'
    'Component 2':
      'Component 2.1': 'docs/partials/elements/link/menu.html'
      'Component 2.2': '#component-2-2'
      'Component 2.3':
        'Component 2.3.1': '#component-1-3-1'
        'Component 2.3.2': '#component-1-3-2'
        'Component 2.3.3': '#component-1-3-3'
agency-pkg:
  headline:
    text: 'agency-pkg'
  list:
    'Component 1': 'docs/partials/elements/link/menu.html'
    'Component 2':
      'Component 2.1': 'docs/partials/elements/link/menu.html'
      'Component 2.2': '#component-2-2'
      'Component 2.3':
        'Component 2.3.1': '#component-1-3-1'
        'Component 2.3.2': '#component-1-3-2'
        'Component 2.3.3': '#component-1-3-3'
---
<div class="pkg controller" data-pkg="agency-pkg-docs/overview/menu" data-controller="agency-pkg-docs/overview/menu" data-target="[data-pkg='agency-pkg-docs/overview']">
  {{{mixin "agency-pkg-docs/overview/toggle" class="js-click-toggle"}}}
  <div>
    <div class="scroll-wrapper">
      <div class="scroll-container">
          {{#block "content"}}

            <div class="src">
              {{{mixin "agency-pkg-docs/overview/headline" src.headline}}}
              {{{mixin "agency-pkg-docs/accordeon" parent-id="src" list=src.list}}}
            </div>

            <div class="agency-pkg">
              {{{mixin "agency-pkg-docs/overview/headline" agency-pkg.headline}}}
              {{{mixin "agency-pkg-docs/accordeon" parent-id="agency-pkg" list=agency-pkg.list}}}
            </div>

          {{/block}}
      </div>
    </div>
  </div>
</div>
